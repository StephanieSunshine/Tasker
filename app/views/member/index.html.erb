<div class="h1 display-4 text-center mx-auto">Tasks</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Status</th>
      <th scope="col">Title</th>
      <th scope="col">Assigned To</th>
      <th scope="col">Created By</th>
      <th scope="col">Last Update</th>
      <th scope="col">Age</th>
    </tr>
  </thead>
  <tbody id='task-list'>
  </tbody>
</table>

<%= render 'modals/add_task_to_queue' %>
<%= render 'modals/task_details' %>
<% if user_is_tech? %>
  <%= render 'modals/next_task' %>
<% end %>
